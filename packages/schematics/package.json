{
  "name": "@kentan-official/schematics",
  "version": "1.5.1",
  "description": "Schematics for @kentan-official/core automating creation of sketches",
  "repository": {
    "type": "git",
    "url": "https://github.com/kentan-official/kentan"
  },
  "scripts": {
    "build:prod": "ts-node -P ./build/tsconfig.json ./build",
    "build:watch": "nodemon",
    "test": "jest",
    "test:prod": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "tsc:prod": "tsc -p tsconfig.build.json",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "schematics",
    "typescript",
    "angular",
    "angular-cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Gregor Woiwode <gregor.woiwode@gmail.com>",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "((\\.|/)(spec))\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  },
  "dependencies": {
    "@angular-devkit/core": "^0.5.10",
    "@angular-devkit/schematics": "^0.5.10",
    "find": "^0.2.9",
    "injection-js": "^2.2.1",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/find": "^0.2.0",
    "@types/fs-extra": "^5.0.2",
    "@types/globby": "^6.1.0",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.0",
    "coveralls": "^3.0.1",
    "fs-extra": "^5.0.0",
    "globby": "^8.0.1",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3"
  }
}
