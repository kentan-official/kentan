{
  "name": "kentan-official",
  "private": true,
  "version": "1.1.2",
  "author": "Gregor Woiwode <gregor.woiwode@gmail.com>",
  "description": "This small framework has the goal to provide a more efficient way to generate test data.",
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "typescript",
    "test",
    "generator"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "test": "lerna run test:prod",
    "build": "lerna run build:prod",
    "report-coverage": "lerna run report-coverage",
    "prerelease": "lerna publish --git-remote mono --skip-npm",
    "release": "lerna exec -- npm publish packages/\\$LERNA_PACKAGE_NAME/dist"
  },
  "devDependencies": {
    "lerna": "^3.0.0-rc.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kentan-official/kentan.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kentan-official/kentan/issues"
  },
  "homepage": "https://github.com/kentan-official/kentan#readme"
}
